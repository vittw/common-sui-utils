{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": [
      "./esm/sui-utils.js",
      "./README.md"
    ]
  },
  "opts": {
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "includeDate": false,
      "staticFiles": {
        "include": [
          "./static"
        ]
      }
    }
  },
  "docdash": {
    "sort": true,
    "search": true,
    "private": false,
    "meta": {
      "title": "申通通用工具函数",
      "description": "提供常见的工具函数集合"
    },
    "scripts": [
      "./index.css"
    ]
  }
}
